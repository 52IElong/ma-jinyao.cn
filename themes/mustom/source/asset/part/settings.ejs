<div p-settings>
  <div p-settings-caption>
    <%- theme.settingsicon %>
    <span data-lang="settings.caption"></span>
  </div>
  <div p-settings-items>
    <% $forIn(theme.settings, (key, icon) => { %>
    <div data-settings-key="<%- key %>">
      <%- icon %>
      <div p-settings-items-switch></div>
      <span p-settings-items-name data-lang="settings.items.<%- key %>"></span>
    </div>
    <% }); %>
  </div>
</div>