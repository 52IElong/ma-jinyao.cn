<%- partial('_partial/frame', {
  favicon: theme.favicon.small,
  manifest: theme.manifest.path,
  parts: JSON.stringify([
    'post'
  ]),
  title: (page.title && page.title) + ' - ' + config.title,
  author: config.author,
  keywords: (o => {
    let result = [];
    page.tags && (result = result.concat(page.tags.map(tag => tag.name)));
    page.categories && (result = result.concat(page.categories.map(cat => cat.name)));
    return result.join(', ');
  })(),
  description: (page.content && page.content.replace(/(<\/?.+?\/?>)+/g, '|').replace('|', '...').substr(0, 147) + '...'),
  google_site_verification: config.google_site_verification,
  baidu_site_verification: config.baidu_site_verification
}) %>